{
  "name": "trackplayer",
  "version": "1.1.0",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format:write": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky install",
    "clean:android": "(cd android && ./gradlew clean)",
    "start:nocache": "yarn start --reset-cache",
    "android:release": "(cd android && ./gradlew assembleRelease)",
    "ios:pods": "(cd ios && pod install)",
    "gen:i18n": "./src/services/i18n/utils/generate-i18n-keys.ts",
    "e2e:android": "echo 1 | maestro test e2e/android",
    "e2e:ios": "echo 2 | maestro test e2e/ios"
  },
  "dependencies": {
    "@faker-js/faker": "^7.6.0",
    "@react-native-async-storage/async-storage": "^1.17.11",
    "@react-native-community/cli": "10.2.0",
    "@react-native-community/cli-platform-android": "10.1.3",
    "@react-native-community/cli-platform-ios": "10.2.1",
    "@react-navigation/bottom-tabs": "^6.5.7",
    "@react-navigation/native": "^6.1.2",
    "@react-navigation/native-stack": "^6.9.8",
    "axios": "^1.3.4",
    "i18next": "^22.4.9",
    "lodash.debounce": "^4.0.8",
    "luxon": "^3.2.1",
    "native-base": "^3.4.25",
    "react": "18.1.0",
    "react-i18next": "^12.1.4",
    "react-native": "0.70.6",
    "react-native-color-picker": "^0.6.0",
